    "dialogueList" : [
        {
            "dialogue" : {
                "EncounterObjectGuid" : "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
            },
            "name" : "Dialogue_MissionStart",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "How is anybody supposed to find them in all this dust?",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "I'm surprised the <i>Scorpions</i> can even operate. They must be blinder than we are.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_YangDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Good hunting, Commander. Oliveira out.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
            ]
        },
        {
            "dialogue" : {
                "EncounterObjectGuid" : "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
            },
            "name" : "Dialogue_MissionSuccess",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "Have you stopped playing with your food Commander? There are more pressing encounters to be getting on with.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_KuritaRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Yamaguchi, I hope your people are coming to pick my pilots up. This would have been so much easier to co-ordinate if we could use our Company.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "What! And have the local Governor bleating all the way to Central Command. That would not be advisable.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_KuritaRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Just seems a waste throwing your 'Mechs in the grinder where we could do this much more efficently.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "For hefty fee no doubt? I'd like to keep this 'off the books' so to speak.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_KuritaRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Your funeral Reynauld...",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
            ]
        },
        {
            "dialogue" : {
                "EncounterObjectGuid" : "d3d33d95-9ed7-4686-b9eb-954ebe51cc02"
            },
            "name" : "Dialogue_MissionFailure",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "A handful of scrappy tanks and you can't even see to that. I'm not so sure you can handle the rest. But, I'll leave that up to you Commander, it's your people in harms way, not mine.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_KuritaRepDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "That's enough; I'm pulling the plug on this operation, Commander.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
            ]
        },
        {
            "dialogue" : {
                "EncounterObjectGuid" : "33166b96-5af2-4724-b970-7884ec41ffd0"
            },
            "name" : "Dialogue_AmbushWarning",
            "overrideDialogueBucketId" : "DiaBkt_SN_1_AmbushWarning",
            "dialogueContent" : [
                {
                    "words" : "PLACEHOLDR: Lucky they are just light tanks Commander! Keep it up!",
